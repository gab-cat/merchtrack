# Environment
NODE_ENV=development
NEXT_PUBLIC_NODE_ENV=${NODE_ENV}
APP_ENV=${NODE_ENV}
NEXT_TELEMETRY_DISABLED=1
NEXT_RUNTIME=nodejs

# Database
# Format: postgresql://user:password@host:port/database
DATABASE_URL=

# Clerk
# Warning: Keep these values secure and never commit actual keys
# Format: sk_test_... or sk_live_...
CLERK_SECRET_KEY=
# Format: pk_test_... or pk_live_...
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=

# Sentry
# Required for source maps upload
SENTRY_AUTH_TOKEN=
# Format: https://xxx@xxx.ingest.sentry.io/xxx
SENTRY_DSN=
NEXT_PUBLIC_SENTRY_DSN=
# Value between 0 and 1, e.g., 0.1 for 10%
SENTRY_TRACES_SAMPLE_RATE=
SENTRY_SUPPRESS_TURBOPACK_WARNING=

# Datadog
NEXT_PUBLIC_DATADOG_APPLICATION_ID=
NEXT_PUBLIC_DATADOG_CLIENT_TOKEN=
NEXT_PUBLIC_DATADOG_SITE=
NEXT_PUBLIC_DATADOG_SERVICE_NAME=

# Cloudflare
NEXT_PUBLIC_CF_BEACON_TOKEN=